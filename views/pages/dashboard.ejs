<%- include('../partials/head'); %>
    <%- include('../partials/header'); %>
        <%- include('../partials/nav'); %>



            <!--========== CONTENTS ==========-->
            <main>
                <section>
                    <h1>Data User</h1>
                    <hr>
                    <button>Tambah</button>
                    <div class="container">
                        <table class="table">
                            <thead>
                                <tr>
                                    <th scope="col">No</th>
                                    <th scope="col">ID</th>
                                    <th scope="col">Username</th>
                                    <th scope="col">Email</th>
                                    <th scope="col">Password</th>
                                </tr>
                            </thead>
                            <tbody>
                                <% var i=1; users.forEach(function(user){ %>
                                    <tr>
                                        <td>
                                            <%= i %>
                                        </td>
                                        <td>
                                            <%= user._id %>
                                        </td>
                                        <td>
                                            <%= user.username %>
                                        </td>
                                        <td>
                                            <%= user.email %>
                                        </td>
                                        <td>
                                            <%= user.password %>
                                        </td>
                                    </tr>
                                    <% i++ }) %>
                                        </br>
                            </tbody>
                        </table>
                    </div>
                </section>
            </main>

            <!--========== MAIN JS ==========-->
            <%- include('../partials/footer'); %>